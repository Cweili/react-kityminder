{
  "name": "react-kityminder",
  "version": "0.0.4",
  "description": "Mind map for react, based on kityminder.",
  "keywords": [
    "mind",
    "map",
    "mindmap",
    "minder",
    "kity",
    "kityminder",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Cweili/react-kityminder.git"
  },
  "author": "Cweili",
  "license": "MIT",
  "homepage": "https://github.com/Cweili/react-kityminder#readme",
  "bugs": {
    "url": "https://github.com/Cweili/react-kityminder/issues"
  },
  "main": "dist/react-kityminder.js",
  "umd:main": "dist/react-kityminder.umd.js",
  "module": "dist/react-kityminder.esm.js",
  "esmodule": "dist/react-kityminder.modern.js",
  "unpkg": "dist/react-kityminder.umd.js",
  "jsdelivr": "dist/react-kityminder.umd.js",
  "source": "src/index.js",
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --jsx createElement --no-compress --globals react=React,kityminder-core=kityminder",
    "build:dev": "cross-env microbundle -i example/index.jsx --jsx createElement --no-compress --format iife --external react,react-dom,kity,kityminder-core --globals react=React,react-dom=ReactDOM,kityminder-core=kityminder --define process.env.NODE_ENV=\"\"",
    "dev": "npm run build:dev -- watch",
    "serve": "sirv -D",
    "lint": "eslint . --fix",
    "clean": "rimraf dist",
    "e2e": "jest --runInBand",
    "start": "run-p dev serve",
    "test": "run-s build:dev e2e",
    "prepare": "run-s clean build"
  },
  "dependencies": {
    "kity": "^2.0.4",
    "kityminder-core": "^1.4.50"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-jest": "^26.3.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "expect-puppeteer": "^4.4.0",
    "jest": "^26.5.0",
    "jest-dev-server": "^4.4.0",
    "jest-puppeteer": "^4.4.0",
    "microbundle": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^2.1.1",
    "puppeteer-to-istanbul": "^1.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "sirv-cli": "^1.0.6"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  }
}
